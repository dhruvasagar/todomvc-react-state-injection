{
  "name": "TodoMVC",
  "version": "1.0.0",
  "description": "Todo MVC using React & Immstruct",
  "repository": "https://github.com/tarkalabs/todomvc",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "lint": "eslint client/**/*.js tests/**/*.js",
    "client-dev": "node client-dev-server.js",
    "start": "nodemon --watch index.js --watch app",
    "postinstall": "webpack",
    "test": "mocha --compilers js:babel/register -r tests/setup tests/**/*_test.js",
    "test-cov": "istanbul cover _mocha -- --compilers js:babel/register  -r tests/setup  tests/**/*_test.js"
  },
  "author": "Dhruva Sagar",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "*",
    "eslint-plugin-react": "*",
    "file-loader": "*"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "immstruct": "^2.0.0",
    "immutable": "^3.8.0",
    "jade": "^1.11.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "rx": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">= 4.1.0"
  }
}
